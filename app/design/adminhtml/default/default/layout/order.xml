<?xml version="1.0"?>
<layout>
    <order_adminhtml_order_index>
        <reference name="content">
            <block type="order/adminhtml_order" name="order_grid" />
        </reference>
    </order_adminhtml_order_index>
    <order_adminhtml_order_cart_index>
        <reference name="head">
            <action method="addJs">
                <script>ccc/jquery-3.6.0.js</script>
            </action>
            <action method="addJs">
                <script>ccc/after_jquery.js</script>
            </action>
            <action method="addJs">
                <script>ccc/mage.js</script>
            </action>
        </reference>
        <reference name="content">
            <block type="order/adminhtml_order_cart_main" name="cart_index">
                <block type="order/adminhtml_order_cart_product" name="product" template="order/cart/product.phtml">
                    <block type="order/adminhtml_order_cart_product_grid" name="product_grid" />
                </block>
            </block>
        </reference>
    </order_adminhtml_order_cart_index>

</layout>